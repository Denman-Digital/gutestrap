{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "gutestrap/col",
	"title": "Row Column",
	"category": "gutestrap-grid",
	"parent": ["gutestrap/row"],
	"icon": "columns",
	"description": "GuteStrap Row Columns are the building blocks of responsive layouts.",
	"keywords": ["layout", "column", "responsive"],
	"version": "1.11",
	"textdomain": "gutestrap",
	"attributes": {
		"width": {
			"type": "object",
			"default": {
				"xs": 12
			}
		},
		"offset": { "type": "object" },
		"alignment": { "type": "object" },
		"contentAlignment": { "type": "object" },
		"background": { "type": "object" },
		"hasBorderRadius": {"type": "boolean"},
		"hasDropShadow": {"type": "boolean"},
		"_isExample": { "type": "boolean" }
	},
	"supports": {
		"anchor": true,
		"alignWide": false,
		"color": {
			"gradients": true,
			"background": true,
			"text": true
		},
		"spacing": {
			"margin": ["top", "bottom"],
			"padding": true
		},
		"dimensions": {
			"minHeight": true
		},
		"defaultStylePicker": false,
		"renaming": false,
		"__experimentalBorder": {
			"color": true,
			"radius": false,
			"style": false,
			"width": true,
			"__experimentalSkipSerialization": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": false,
				"style": false,
				"width": true
			}
		}
	},
	"styles": [],
	"example": {
		"attributes": {
			"_isExample": true
		},
		"innerBlocks": [
			{
				"name": "core/image",
				"attributes": {
					"sizeSlug": "large",
					"url": "https://raw.githubusercontent.com/Denman-Digital/gutestrap/main/assets/block-preview-column.png"
				}
			}
		]
	},
	"editorStyle": "gutestrap-block-editor-css",
	"editorScript": "gutestrap-block-js"
}
