{
	"name": "gutestrap",
	"displayName": "Gutestrap",
	"version": "2.0.0-compat",
	"private": true,
	"author": "Denman Digital",
	"description": "Supercharge your Gutenberg layouts with Bootstrap Grid.",
	"repository": {
		"type": "git",
		"url": "https://github.com/Denman-Digital/gutestrap"
	},
	"homepage": "https://github.com/Denman-Digital/gutestrap/tree/compat#readme",
	"dependencies": {
		"@pwalton/js-utils": "^2.0.0",
		"@wordpress/compose": "^3.22.0",
		"@wordpress/icons": "^2.8.0",
		"bootstrap": "4.*",
		"bootstrap-icons": "^1.1.0",
		"classnames": "^2.3.2",
		"lodash": "^4.17.21",
		"sprintf-js": "^1.1.2"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-transform-block-scoping": "^7.20.11",
		"@babel/plugin-transform-parameters": "^7.20.7",
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/register": "^7.18.9",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.0.0",
		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-replace": "^5.0.2",
		"@rollup/plugin-terser": "^0.3.0",
		"@svgr/rollup": "^6.5.1",
		"acorn": "^7.4.1",
		"autoprefixer": "^9.7.4",
		"babel-plugin-direct-import": "^1.0.0",
		"chalk": "^4.*",
		"cross-env": "^7.0.3",
		"del": "^7.0.0",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-compat": "^4.0.2",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.1",
		"fancy-log": "^2.0.0",
		"flat-map": "^1.0.0",
		"glob": "^8.1.0",
		"gulp": "^4.0.2",
		"gulp-babel": "^8.0.0",
		"gulp-clean-css": "^4.3.0",
		"gulp-eslint-new": "^1.7.1",
		"gulp-filter": "^7.0.0",
		"gulp-if": "^3.0.0",
		"gulp-imagemin": "^7.1.0",
		"gulp-plumber": "^1.2.1",
		"gulp-postcss": "^9.0.1",
		"gulp-rename": "^2.0.0",
		"gulp-replace": "^1.1.4",
		"gulp-sass": "^5.1.0",
		"gulp-scale-images": "^3.0.2",
		"gulp-size": "^4.0.1",
		"gulp-string-replace": "^1.1.2",
		"humanize-duration": "^3.28.0",
		"lazypipe": "^1.0.2",
		"path": "^0.12.7",
		"prettier": "^2.8.3",
		"readline-sync": "^1.4.10",
		"rollup": "^3.10.0",
		"rtlcss": "^4.0.0",
		"sass": "^1.57.1",
		"through2": "^3.0.1"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			[
				"@babel/preset-react",
				{
					"runtime": "automatic"
				}
			]
		],
		"plugins": [
			"@babel/plugin-proposal-class-properties",
			"@babel/plugin-transform-parameters",
			"@babel/plugin-transform-block-scoping",
			[
				"babel-plugin-direct-import",
				{
					"modules": [
						"@pwalton/js-utils"
					]
				}
			]
		]
	},
	"rtlcssConfig": {
		"options": {
			"autoRename": false,
			"autoRenameStrict": false,
			"blacklist": {},
			"clean": true,
			"greedy": false,
			"processUrls": false,
			"stringMap": []
		},
		"plugins": [],
		"map": false
	},
	"browserslist": [
		"cover 99.75% in CA",
		">= 0.5%",
		"last 2 major versions",
		"last 5 versions and not IE < 11",
		"last 3 Chrome major versions",
		"last 3 Safari major versions",
		"last 3 Firefox major versions",
		"last 3 Edge major versions",
		"last 5 ChromeAndroid major versions",
		"last 5 iOS major versions",
		"last 5 FirefoxAndroid major versions",
		"last 5 Samsung major versions",
		"Firefox ESR"
	],
	"scripts": {
		"lint": "gulp lint --color",
		"test": "npm run lint",
		"start": "cross-env NODE_ENV=development gulp dev --color",
		"build": "cross-env NODE_ENV=production gulp build --color",
		"build:dist": "cross-env NODE_ENV=production gulp build --color && composer install --no-dev",
		"build:styles": "cross-env NODE_ENV=production gulp styles --color",
		"build:scripts": "cross-env NODE_ENV=production gulp scripts --color"
	}
}
